{
    "name": "acecursor",
    "displayName": "AceCursor",
    "description": "Smarter jumps but Zero Vim",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.107.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "acecursor.JumpToOpeningBraceUp",
                "title": "Cursor: Jump To { UP"
            },
            {
                "command": "acecursor.JumpToOpeningBraceDown",
                "title": "Cursor: Jump To { DOWN"
            },
            {
                "command": "acecursor.JumpToOpeningBrace",
                "title": "Cursor: Jump To CLOSEST { "
            },
            {
                "command": "acecursor.JumpToClosingBraceUp",
                "title": "Cursor: Jump To } UP"
            },
            {
                "command": "acecursor.JumpToClosingBraceDown",
                "title": "Cursor: Jump To } Down"
            },
            {
                "command": "acecursor.JumpToClosingBrace",
                "title": "Cursor: Jump To CLOSEST }"
            },
            {
                "command": "acecursor.JumpToOpeningParenthesisUp",
                "title": "Cursor: Jump To ( UP"
            },
            {
                "command": "acecursor.JumpToOpeningParenthesisDown",
                "title": "Cursor: Jump To ( DOWN"
            },
            {
                "command": "acecursor.JumpToOpeningParenthesis",
                "title": "Cursor: Jump To CLOSEST ("
            },
            {
                "command": "acecursor.JumpToClosingParenthesisUp",
                "title": "Cursor: Jump To ) UP"
            },
            {
                "command": "acecursor.JumpToClosingParenthesisDown",
                "title": "Cursor: Jump To ) Down"
            },
            {
                "command": "acecursor.JumpToClosingParenthesis",
                "title": "Cursor: Jump To CLOSEST )"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "pnpm run package",
        "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.107.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "typescript-eslint": "^8.48.1",
        "eslint": "^9.39.1",
        "esbuild": "^0.27.1",
        "npm-run-all": "^4.1.5",
        "typescript": "^5.9.3",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2"
    }
}
